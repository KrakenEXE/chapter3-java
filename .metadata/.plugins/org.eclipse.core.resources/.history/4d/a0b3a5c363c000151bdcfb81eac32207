package projectFour;

public class TemperatureDetermination {
	//create variables
	private double ftemp;

	//create constructor
	public TemperatureDetermination(double inputFTemp){
		ftemp = inputFTemp;
	}
	
	//create get set | accessors mutators
	public double getFtemp() {
		return ftemp;
	}
	
	public void setFtemp(double ftemp) {
		this.ftemp = ftemp;
	}
	
}
